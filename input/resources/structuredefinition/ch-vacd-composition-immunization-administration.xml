      <element id="Composition.section:annotation">
         <path value="Composition.section"/>
         <sliceName value="annotation"/>
         <short value="Annotation"/>
         <min value="0"/>
         <max value="1"/>
      </element>
      <element id="Composition.section.title:annotation">
